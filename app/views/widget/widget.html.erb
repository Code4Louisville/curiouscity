<div class="widget-prompt">
Which would you rather know?    
</div>
<div class="questions-container">
  <% @questions.each do |question| %>
    <div class="widget-question">
      <%= question.display_text %>
      <%= link_to "", vote_path(question_id: question.id, voting_round_id: @voting_round.id), method: :post, class: "vote", id: "vote"+question.id.to_s %>
    </div>
  <% end %>
</div>
