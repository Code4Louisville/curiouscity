<h1>Listing questions</h1>

<table>
    <tr>
      <th>Display text</th>
      <th>Name</th>
    </tr>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.display_text %></td>
        <td><%= if question.anonymous then "Anonymous" else question.name end%></td>
        <td><%= link_to 'Show', admin_question_path(question) %></td>
      </tr>
    <% end %>
</table>

<br>
